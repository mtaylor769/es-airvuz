<!DOCTYPE html>
<html view="app.views.view.template.static">
    <head>
        <meta http-equiv="Cache-control" content="public">
        <title>{title}</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" href="/client/favicon.ico" type="image/x-icon">
        <link rel="stylesheet" href="{script.css}">
    </head>
    <body>
        {> "app.views.view.partial.header" /}

        <div id="static-page" class="container main">
            {> "app.views.view.partial.{staticPage}" /}
        </div>

        {> "app.views.view.partial.footer" /}
        {> "app.views.view.partial.js" /}
        <script>
          $(function () {
            Static.initialize();
          });
        </script>
    </body>

</html>
