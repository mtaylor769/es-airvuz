<!DOCTYPE html>
<html view="app.views.view.template.index">
  <head>
        <meta http-equiv="Cache-control" content="public">

        <title>{index.head.title}</title>

        <!-- BEGIN: OG -->
        <meta property="og:url" content="{index.head.og.url}"/>
        <meta property="og:type" content="{index.head.og.type}" >
        <meta property="og:title" content="{index.head.og.title}" >
        <meta property="og:updated_time" content="{index.head.og.updated_time}">
        <meta property="og:description" content="{index.head.og.description}" >
        <meta property="og:image" content="{index.head.og.image}" >
        <meta property="fb:app_id" content="{index.fb.app_id}">
        <!-- END: OG -->

        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        {> "app.views.view.partial.css" /}
  </head>
  <body>
  
    {> "app.views.view.partial.header" /}

    <!-- BEG:Main Container -->
    <div id="home-page" class="container main container-shadow">

      {> "app.views.view.partial.home-video" title="Featured Videos" videos="featuredVideos"/}
      <hr/>
      {> "app.views.view.partial.home-video" title="Recent Videos" videos="recentVideos"/}
      <hr/>
      {> "app.views.view.partial.home-video" title="Trending Videos" videos="trendingVideos"/}
      <br/>
      <br/>
    </div>
    <!-- END:Main Container -->

    {> "app.views.view.partial.footer" /}

    {> "app.views.view.partial.js" /}

    <script>
        $(function () {
          Home.initialize();
        });
    </script>
  </body>
    
</html>
