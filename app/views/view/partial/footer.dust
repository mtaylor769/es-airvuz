<!-- Modal -->
<div class="modal fade" id="login-modal" tabindex="-1" role="dialog">
  <div class="modal-dialog modal-sm" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">Login</h4>
      </div>
      <div class="modal-body">
        <div class="tab-content">
          <div role="tabpanel" class="tab-pane active" id="login-tab">
              <div class="social-panel">
                  <button class="btn btn-block btn-primary facebook" id="btn-facebook">Facebook</button>
                  <button class="btn btn-block btn-danger" id="btn-google">Google</button>
                  <p class="text-danger text-center m-t-5" id="social-login-error"></p>
                  <hr/>
              </div>
              <div class="local-panel">
                  <form>
                      <div class="form-group">
                          <label>Email <span class="text-danger">*</span></label>
                          <input type="text" class="form-control email-input" placeholder="Email">
                      </div>
                      <div class="form-group">
                          <label>Password <span class="text-danger">*</span></label>
                          <input type="password" class="form-control password-input" placeholder="Password">
                      </div>
                      <p class="error-message text-danger"></p>
                      <div class="text-right">
                          <button type="submit" class="btn btn-primary" id="login-btn">Login</button>
                      </div>
                  </form>
                  <hr/>
                  <div class="text-center">
                      <a href="#signup-tab" aria-controls="signup-tab" role="tab" data-toggle="tab" id="signup-anchor-tab">Sign up</a> | <a href="#forgot-password-tab" aria-controls="forgot-password-tab" role="tab" data-toggle="tab">Forgot password</a>
                  </div>
              </div>
          </div>
          <div role="tabpanel" class="tab-pane" id="signup-tab">
            <div class="form-group">
              <label>Email <span class="text-danger">*</span></label>
              <input type="text" id="email" class="form-control" placeholder="Email">
            </div>
            <div class="form-group">
                <label>Username <span class="text-danger">*</span></label>
                <input type="text" id="username" class="form-control" placeholder="Username">
            </div>
            <div class="form-group">
              <label>Password <span class="text-danger">*</span></label>
              <input type="password" id="password" class="form-control" placeholder="Password">
            </div>
            <div class="form-group">
                <label>Confirm Password <span class="text-danger">*</span></label>
                <input type="password" id="confirm-password" class="form-control" placeholder="Confirm Password">
            </div>
            <div class="form-group">
              <label for="isSubscribeAirVuzNews">Subscribe to AirVuz News</label><br/>
              <input type="checkbox" id="isSubscribeAirVuzNews" value="true" checked>
            </div>
            <div class="text-message text-success hidden">
                <p>You signed up successfully. Please check your inbox to confirm.</p>
            </div>
            <div class="text-right">
              <button type="button" class="btn btn-primary" id="signup-btn">Sign up</button>
            </div>
            <hr/>
            <div class="text-center">
              <a href="#login-tab" aria-controls="login-tab" role="tab" data-toggle="tab" id="login-anchor-tab">Login</a> | <a href="#forgot-password-tab" aria-controls="forgot-password-tab" role="tab" data-toggle="tab">Forgot password</a>
            </div>
          </div>
          <div role="tabpanel" class="tab-pane" id="forgot-password-tab">
            <p>Enter your email address below and we'll send you password reset instructions.</p>
            <div class="form-group">
              <label>Email <span class="text-danger">*</span></label>
              <input type="text" class="form-control email-input" placeholder="Email">
            </div>
            <div class="text-message text-success hidden">
              <p>Email sent, Please check your mailbox</p>
            </div>
            <div class="text-message text-danger hidden">
                <p>There was a problem resetting your password or incorrect email</p>
            </div>
            <div class="text-right">
              <button type="button" class="btn btn-primary" id="btn-password-reset">Reset</button>
            </div>
            <hr/>
            <div class="text-center">
              <a href="#login-tab" aria-controls="login-tab" role="tab" data-toggle="tab">Login</a> | <a href="#signup-tab" aria-controls="signup-tab" role="tab" data-toggle="tab">Sign up</a>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
