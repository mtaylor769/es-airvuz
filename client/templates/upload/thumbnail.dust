<label>Select a thumbnail <span class="text-danger">*</span></label>
<input type="hidden" name="thumbnailPath"/>
<ul>
	{#thumbnails}
	<li data-url="{.}" style="background-color: gray; background-image: url('{url}{.}')" {@eq key=. value=selectedThumbnail}class="active"{/eq} >
	</li>
	{/thumbnails}
</ul>