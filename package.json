    "cssnano": "^3.5.2",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "es6-promise": "3.1.2",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "isomorphic-fetch": "2.2.1",
    "jsonwebtoken": "^5.7.0",
    "lodash": "4.5.0",
    "log4js": "^0.6.32",
    "lost": "^6.7.2",
    "mongoose": "^4.4.4",
    "morgan": "^1.6.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-google-oauth": "^1.0.0",
