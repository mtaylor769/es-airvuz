{
  "name": "AirVuz2",
  "version": "2.0.0",
  "scripts": {
    "start": "node server.js",
    "start:cluster": "node cluster.js",
    "start:server-debug": "node-debug --web-host 127.0.0.1 server.js",
    "start:server-debug-nobrk": "node-debug --debug-brk=0 --web-host 127.0.0.1 server.js",
    "start:server-debug-win": "node-debugs server.js",
    "build": "node node_modules/webpack/bin/webpack.js --config client/webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --config client/webpack.config.js",
    "build:win": "set NODE_ENV=development&& webpack --config client/webpack.config.js",
    "build:prod:win": "set NODE_ENV=production&& webpack --config client/webpack.config.js",
    "test": "npm run test:node && npm run test:karma -- --single-run",
    "test:node": "JASMINE_CONFIG_PATH=specs/support/jasmine.app.json istanbul cover --dir ./coverage/app -x **/specs/** jasmine",
    "test:jasmine": "JASMINE_CONFIG_PATH=specs/support/jasmine.app.json jasmine",
    "test:karma": "karma start",
    "test:win": "npm run test:node:win && npm run test:karma:win -- --single-run",
    "test:node:win": "set  JASMINE_CONFIG_PATH=specs/support/jasmine.app.json&& istanbul cover --dir ./coverage/app -x **/specs/** jasmine",
    "test:jasmine:win": "set  JASMINE_CONFIG_PATH=specs/support/jasmine.app.json&& jasmine",
    "test:karma:win": "karma start",
    "test:api:local" : "NODE_TEST_ENV=localhost:3000 TEST_EMAIL=bryceisworking@gmail.com TEST_USER=testingUser TEST_PWD= mocha testV2/api/*.js --reporter spec",
    "test:api:beta" : "NODE_TEST_ENV=beta.airvuz.com TEST_EMAIL=bryceisworking@gmail.com TEST_USER=testingUser TEST_PWD= mocha testV2/api/*.js --reporter spec",
    "test:api:prod" : "NODE_TEST_ENV=www.airvuz.com TEST_EMAIL=bryceisworking@gmail.com TEST_USER=testingUser TEST_PWD= mocha testV2/api/*.js --reporter spec"
  },
